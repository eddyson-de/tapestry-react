	t:type="Layout" title="Alerts Component Demo" xmlns:r="tapestry-library:react">
	<div class="row">
		<div class="col-sm-12">
			<r:reactcomponent module="eddyson/react/components/react/Alerts" />
		</div>
		<r:reactcomponent module="eddyson/react/components/react/EventLink" event="hello">Hello Tapestry!</r:reactcomponent>
		
		<r:reactcomponent module="eddyson/react/components/react/EventLink" event="addalert" async="prop:true">Say hello Roger!</r:reactcomponent>

	</div>
</html>